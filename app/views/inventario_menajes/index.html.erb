<h1>Listado de inventario_menajes</h1>

<table class="table table-striped">
  <tr>
    <th>Escuela</th>
    <th>Ultima modificacion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inventario_menajes.each do |inventario_menaje| %>
  <tr>
    <td><%= inventario_menaje.escuela_id %></td>
    <td><%= inventario_menaje.ultima_modificacion %></td>
    <td><%= link_to 'ver', inventario_menaje %></td>
    <td><%= link_to 'editar', edit_inventario_menaje_path(inventario_menaje) %></td>
    <td><%= link_to 'eliminar', inventario_menaje, method: :delete, data: { confirm: 'Desea eliminar?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Agregar Inventario menaje', new_inventario_menaje_path %>
