<h1>Listado de inventario_equipamientos</h1>

<table class="table table-striped">
  <tr>
    <th>Escuela</th>
    <th>Ultima modificacion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inventario_equipamientos.each do |inventario_equipamiento| %>
  <tr>
    <td><%= inventario_equipamiento.escuela_id %></td>
    <td><%= inventario_equipamiento.ultima_modificacion %></td>
    <td><% if can? :read, InventarioEquipamiento %><%= link_to 'ver', inventario_equipamiento %><% end %></td>
    <td><% if can? :update, InventarioEquipamiento %><%= link_to 'editar', edit_inventario_equipamiento_path(inventario_equipamiento) %><% end %></td>
    <td><% if can? :destroy, InventarioEquipamiento %><%= link_to 'eliminar', inventario_equipamiento, method: :delete, data: { confirm: 'Seguro desear eliminar?' } %><% end %></td>
  </tr>
<% end %>
</table>

<br />

<% if can? :create, InventarioEquipamiento %><%= link_to 'Agregar Inventario equipamiento', new_inventario_equipamiento_path %><% end %>
