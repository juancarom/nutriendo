<h1>Listing inventario_equipamientos</h1>

<table>
  <tr>
    <th>Escuela</th>
    <th>Ultima modificacion</th>
    <th>Quien modifica</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inventario_equipamientos.each do |inventario_equipamiento| %>
  <tr>
    <td><%= inventario_equipamiento.escuela_id %></td>
    <td><%= inventario_equipamiento.ultima_modificacion %></td>
    <td><%= inventario_equipamiento.quien_modifica %></td>
    <td><%= link_to 'Show', inventario_equipamiento %></td>
    <td><%= link_to 'Edit', edit_inventario_equipamiento_path(inventario_equipamiento) %></td>
    <td><%= link_to 'Destroy', inventario_equipamiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

