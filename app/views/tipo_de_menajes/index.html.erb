<h1>Listado de tipos de menajes</h1>

<table class="table table-striped">
  <tr>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_de_menajes.each do |tipo_de_menaje| %>
  <tr>
    <td><%= tipo_de_menaje.nombre %></td>
    <td><%= tipo_de_menaje.descripcion %></td>
    <td><%= link_to 'ver', tipo_de_menaje %></td>
    <td><%= link_to 'editar', edit_tipo_de_menaje_path(tipo_de_menaje) %></td>
    <td><%= link_to 'eliminar', tipo_de_menaje, method: :delete, data: { confirm: 'Desea eliminar?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Agregar Tipo de menaje', new_tipo_de_menaje_path %>
