


<table class="showTableBorder">
  <tr>
    <th class="firstCell">Model</th>
    <th class="lastCell">Action</th>
    <th class="lastCell firstCell" colspan="3"></th>
  </tr>
  <%  @role_permissions.each do |role_permission| %>
    <tr>
      <td class="firstCell"><%= role_permission.regulator %></td>
      <td class="middleCell"><%= role_permission.conduct %></td>
      <td class="middleCell"><%= link_to 'ver', role_permission, :class => 'ajaxLink' %></td>
      <td class="middleCell"><%= link_to 'editar', edit_role_permission_path(role_permission), :class => 'ajaxLink' %></td>
      <td class="lastCell"><%= link_to 'eliminar', role_permission_path(role_permission,:role_id => role_permission.role_id), :class => 'ajaxDeleteLink' %></td>
    </tr>
  <%   end %>
</table>

<br />

<%= link_to 'Agregar Role permission', new_role_permission_path(:role_id => @the_role_id), :class => 'ajaxLink' %>
